import java.util.Scanner; 
import java.util.Random; 


public class Guessnumber 
{ 
public static void main(String[] args) 
{ 
    String personPlay;  
    String computerPlay = ""; 
    int computerInt; 
    String response; 


    Scanner scan = new Scanner(System.in); 
    Random generator = new Random(); 

    System.out.println("Let's play guess a number\n");
    System.out.println("Enter your number: "); 
    
    personPlay = scan.nextInt();


    
    System.out.println("Computer play is: " + computerPlay); 

    
    computerInt = generator.nextInt(10)+1; 

    if (personPlay == 1) && (computerPlay==2)
    
	 System.out.println("personPlay is 1 AND computerPlay is 2"); 

    if (personPlay == 1) || (computerPlay==1)
    
	 System.out.println("personPlay is 1 OR computerPlay is 1"); 





    if (personPlay == computerPlay) 
       
	 System.out.println("It's a correct number"); 
    else if (personPlay > computerPlay) 
       
          System.out.println("too big!!!");
    else if (personPlay < computerPlay) 
       
          System.out.println("too small!!!");
    
    else 
        
	  System.out.println("Invalid user input."); 




}
}
